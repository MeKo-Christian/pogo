# ONNX Runtime environment variables
export CGO_CFLAGS="-I$PWD/onnxruntime/include"
export CGO_LDFLAGS="-L$PWD/onnxruntime/lib -lonnxruntime"
export LD_LIBRARY_PATH="$PWD/onnxruntime/lib:$LD_LIBRARY_PATH"

# Optional: Add onnxruntime/lib to library path for runtime
export DYLD_LIBRARY_PATH="$PWD/onnxruntime/lib:$DYLD_LIBRARY_PATH"  # macOS

echo "ONNX Runtime environment loaded"